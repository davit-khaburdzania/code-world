<div class="row">
  <div class="col-md-10 col-md-offset-1">
      <% (1..20).each do |place_id| %>
        <%- if @house=find_place(@houses, place_id) %>
          <div class="place">
            <%= image_tag("house_1.png", :class => "place-img") %>
            <%= link_to @house.title, @house %>
          </div>
        <% else %>
          <a href="/house/new">
            <div class="place">
              <img src="" alt="">
              <span>Create House</span>
            </div>
          </a>
        <% end %>
      <% end %>
  </div>
</div>
